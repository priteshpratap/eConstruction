								</VBox>
								
									<VBox alignItems="Center">
									<Avatar src="{
											path: 'view>/plywood',
											formatter : '.formatter.pictureUrl'
											}" 
											displaySize="XL" 
											displayShape="Circle">
										<detailBox>
											<LightBox>
												<LightBoxItem imageSrc="{
											path: 'view>/plywood',
											formatter : '.formatter.pictureUrl'
											}" alt="Michael Adams" title="Plywood"
													subtitle=""></LightBoxItem>
											</LightBox>
										</detailBox>
									</Avatar>
									<Text textAlign="Center" text="Plywood" class="sapUiSmallMarginTop"/>
								</VBox>
								
								<VBox alignItems="Center">
									<Avatar src="{
											path: 'view>/Labour',
											formatter : '.formatter.pictureUrl'
											}" 
											displaySize="XL" 
											displayShape="Circle">
										<detailBox>
											<LightBox>
												<LightBoxItem imageSrc="{
											path: 'view>/Labour',
											formatter : '.formatter.pictureUrl'
											}" alt="Michael Adams" title="Labour Management"
													subtitle=""></LightBoxItem>
											</LightBox>
										</detailBox>
									</Avatar>
									<Text textAlign="Center" text="Labour Management" class="sapUiSmallMarginTop"/>
								</VBox>
								
								
