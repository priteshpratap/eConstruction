									</Avatar>
									<Text textAlign="Center" text="Gitti - Stone chips" class="sapUiSmallMarginTop"/>
								</VBox>
								
								<VBox alignItems="Center">
									<Avatar src="{
											path: 'view>/IronRod',
											formatter : '.formatter.pictureUrl'
											}" 
											displaySize="XL" 
											displayShape="Circle">
